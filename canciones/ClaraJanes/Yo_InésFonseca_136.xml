         <div>
            <lg type="lyrics">
               <lg>
                  <l>Yo</l>
                  <l> aún</l>
                  <l> aquí</l>
               </lg>
               <lg>
                  <l> la mente</l>
                  <l> perforada</l>
               </lg>
               <lg>
                  <l> mientras</l>
                  <l> tibia devuelve</l>
                  <l> la arena</l>
               </lg>
               <lg>
                  <l> sirenas</l>
                  <l> al oído</l>
               </lg>
               <lg>
                  <l> y pulso</l>
                  <l> a las muñecas</l>
               </lg>
               <lg>
                  <l> y nudo</l>
                  <l> a la garganta</l>
               </lg>
